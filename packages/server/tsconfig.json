{
	"compilerOptions": {
		"outDir": "dist",
		"target": "es6",
		"module": "CommonJS",
		"strict": true,
		"allowJs": true,
		"strictNullChecks": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"allowSyntheticDefaultImports": true,
		"baseUrl": "./src"
		// "paths": {
		// 	"@/*": ["./src"]
		// }
	},
	"include": ["src"]
}
